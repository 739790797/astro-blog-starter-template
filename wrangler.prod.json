{
  "name": "astro-blog-starter-template",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "r2_buckets": [
    {
      "binding": "BLOG_ASSETS",
      "bucket_name": "baiyi-blog-images" 
    }
  ],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}